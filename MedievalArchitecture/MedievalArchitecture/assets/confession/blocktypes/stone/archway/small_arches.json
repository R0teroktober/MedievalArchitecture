{
	"enabled": true,
	"code": "arch-s-ng",
	"maxStackSize": 1,
	"blockmaterial": "Stone",
	"shape": { "base": "confession:block/stone/arch-s-gothic" },
	"textures": { "stone": { "base": "game:block/stone/rock/granite1" } },
	"entityBehaviors": [{ "name": "AttributeRenderingLibrary.ShapeTexturesFromAttributes" }],
	"behaviors": [
		{
			"name": "AttributeRenderingLibrary.BlockShapeTexturesFromAttributes",
			"properties": {
				"name": {
					"*": [ "Gothic archway", " (", "material-{material}", ")" ]
				},
				"textures": {
					"material-glass": {
						"stone": { "base": "game:block/stone/rock/{material}1" },
						"glass": { "base": "game:block/glass/{glass}" }
					},
					"material-noglass": {
						"stone": { "base": "game:block/stone/rock/{material}1" },
						"glass": { "base": "game:block/transparent" }
					}
				}
			}
		}
	],
	"creativeInventoryStacks": [
		{
			// default creative tabs
			"tabs": [ "general", "decorative" ],
			"stacks": [
				// in this case, we add our block with all possible wood types, but we can make our block have more types, for example:
				// "types": {
				//    "wood": "birch",
				//    "stone": "granite",
				//    "metal": "iron"
				// }

				{
					"type": "block",
					"code": "arch-s-ng",
					"attributes": { "types": {"material": "bauxite", "glass":  "red" } }
				},
				{
					"type": "block",
					"code": "arch-s-ng",
					"attributes": { "types": { "material": "andesite", "glass":  "transparent" } }
				},
				{
					"type": "block",
					"code": "arch-s-ng",
					"attributes": { "types": { "material": "limestone", "glass":  "leaded" } }
				}
			]
		}
	]

}